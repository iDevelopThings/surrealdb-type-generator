{
  "include": [
    "./gen/**/*.ts"
  ],
  "compilerOptions": {
    "module": "CommonJS",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "target": "ESNext",
    "rootDir": "./gen",
    "outDir": "./dist/gen",
    "moduleResolution": "Node",
    "strict": false,
    "sourceMap": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationDir": "./dist/gen",
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true,
    "keyofStringsOnly": true
  },
  "exclude": [
    "node_modules"
  ]
}
